  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --watch",
    "gulp": "gulp serve",
    "node": "nodemon server_node.js",
    "swagger": "swagger-codegen generate -i ./specifications/api/swagger.yaml -o ./public_html/swagger/ -l html2",
    "sass:lint": "clear && gulp sass:lint",
    "prism:mockapi": "prism run --mock --list --spec http://localhost:8000/lib/swagger.yaml --config ./specifications/api/prism.conf.json",
    "test": "protractor ./e2e/conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf --outFile ./src/locale/locale.de.xlf -p ./tsconfig.json",
    "ngc:localization": "./node_modules/.bin/ngc --i18nFile=./src/locale/locale.de.xlf --locale=de --i18nFormat=xlf -p ./tsconfig.aot.json",
    "ngc": "./node_modules/.bin/ngc -p ./src/tsconfig.aot.json"
  },
  "private": true,
